    <script type="text/javascript">

        $(function () {
            $(".degreeName").click(function () {

                //var degree = encodeURIComponent($(this).text().trim());
                var degree = encodeURIComponent($(this).text().trim());
                $.ajax({
                    url: "@(Url.Action("Degree", "DegreePlan"))",
                    data: {args : degree},
